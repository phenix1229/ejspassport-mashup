<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head') %>
    <title>Random</title>
</head>
<body>
    <div id="body">
        <div id="header">
            <header>Random Users</header>
        </div>
        <div id="uGallery">
            <% userInfo.forEach(element => { %>
            <div class="uInfoSpace">
                <img src="<%- element[0] %>">
                <%= element[1] %> <%= element[2] %> 
                </div>
            <% }) %>
        </div>
        <div id="footer">
            <%- include('../partials/footer') %>
        </div>
    </div>
</body>
</html>